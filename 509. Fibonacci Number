class Solution {
    public int fib(int n) {
        if(n==0)
            return 0;

        if(n==1)
            return 1;

        int cur=0;
        int prev=1;
        int num=1;
        int next=0;

        while(num <= n)
        {
            next=cur+prev;
            prev=cur;
            cur=next;
            num++;
        }

        return next;
        
    }
}
