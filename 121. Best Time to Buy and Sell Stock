class Solution {
    public int maxProfit(int[] prices) {
    
        int buy=prices[0];
        int maxprofit=-1;
        for(int i=1;i<prices.length;i++)
        {
            int profit=0;
            if((prices[i]-buy)<=0)
            {
                buy=prices[i];
            }
            else{
                profit=prices[i]-buy;
            }
            if(profit>maxprofit)
                maxprofit=profit;
        }

        if( maxprofit > 0)
            return maxprofit;
        else
            return 0;
    }
}
